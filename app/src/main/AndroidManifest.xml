<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.edu.sjtu.valoraner" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="21" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"
            android:label="main_time"></activity>

        <activity android:name=".RegisterActivity"
            android:label="register_time"></activity>

        <activity android:name=".UsrActivity"
            android:label="usr_time"></activity>

        <activity android:name=".MatchActivity"
            android:label="match_time"></activity>

        <activity android:name=".DataActivity"
            android:label="data_time"></activity>

        <activity android:name=".PlayerActivity"
            android:label="player_time"></activity>

        <activity android:name=".HeroActivity"
            android:label="hero_time"></activity>

        <activity android:name=".HeroIntroduce1"
            android:label="hero_time"></activity>

    </application>

</manifest>
